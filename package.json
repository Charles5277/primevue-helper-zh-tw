{
  "name": "quick-view-config",
  "displayName": "quick-view-config",
  "description": "build component view quickly!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quick-view-config.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "componentTree.insertCode",
        "title": "Insert Code Snippet"
      },
      {
        "command": "componentTree.openDoc",
        "title": "Open Document"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "componentTree.insertCode",
          "when": "view == component-tree && viewItem == leafNode",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "components",
          "title": "Components",
          "icon": "image/icon.svg"
        }
      ]
    },
    "views": {
      "components": [
        {
          "id": "component-tree",
          "name": "Component Tree",
          "icon": "image/icon.svg",
          "contextualTitle": "Component Tree"
        }
      ]
    }
  },
  "scripts": {
    "gen-meta": "deno run -A --unstable-sloppy-imports ./scripts/gen-component-config.ts",
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "radash": "^12.1.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  }
}
